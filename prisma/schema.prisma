generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UserInfo {
  id    Int    @id
  type  Int?
  value String?
  @@map("userinfo")
}

model RContact {
  username           String  @id
  alias              String?
  conRemark          String?
  domainList         String?
  nickname           String?
  pyInitial          String?
  quanPin            String?
  showHead           Int?
  type               Int?
  uiType             BigInt?
  weiboFlag          Int?
  weiboNickname      String?
  conRemarkPYFull    String?
  conRemarkPYShort   String?
  lvbuff             Bytes?
  verifyFlag         Int?
  encryptUsername    String?
  chatroomFlag       Int?
  deleteFlag         Int?
  contactLabelIds    String?
  descWordingId      String?
  openImAppid        String?
  sourceExtInfo      String?
  ticket             String?
  usernameFlag       BigInt?
  contactExtra       Bytes?
  createTime         BigInt?
  @@map("rcontact")
}

model Message {
  msgId              Int     @id
  msgSvrId           BigInt?
  type               Int
  status             Int?
  isSend             Int
  isShowTimer        Int?
  createTime         BigInt
  talker             String
  content            String?
  imgPath            String?
  reserved           String?
  lvbuffer           Bytes?
  transContent       String?
  transBrandWording  String?
  talkerId           Int?
  bizClientMsgId     String?
  bizChatId          Int?
  bizChatUserId      String?
  msgSeq             Int?
  flag               Int?
  solitaireFoldInfo  Bytes?
  historyId          String?
  @@map("message")
}

model ChatRoom {
  chatroomname                     String  @id
  addtime                          BigInt?
  memberlist                       String?
  displayname                      String?
  chatroomnick                     String?
  roomflag                         Int?
  roomowner                        String?
  roomdata                         Bytes?
  isShowname                       Int?
  selfDisplayName                  String?
  style                            Int?
  chatroomdataflag                 Int?
  modifytime                       BigInt?
  chatroomnotice                   String?
  xmlChatroomnotice                String?
  chatroomVersion                  Int?
  chatroomnoticeEditor             String?
  chatroomnoticePublishTime        BigInt?
  chatroomNoticeNew                Int?
  chatroomLocalVersion             BigInt?
  chatroomStatus                   Int?
  memberCount                      Int?
  chatroomfamilystatusmodifytime   BigInt?
  associateOpenIMRoomName          String?
  openIMRoomMigrateStatus          Int?
  saveByteVersion                  String?
  handleByteVersion                String?
  roomInfoDetailResByte            Bytes?
  oldChatroomVersion               Int?
  localChatRoomWatchMembers        Bytes?
  spamStatus                       Int?
  compactFlag                      BigInt?
  @@map("chatroom")
}

model ImgFlag {
  username       String  @id
  imgflag        Int?
  lastupdatetime Int?
  reserved1      String?
  reserved2      String?
  reserved3      Int?
  reserved4      Int?
  updateflag     Int?
  @@map("img_flag")
}

model EmojiInfoDesc {
  md5_lang  String @id
  md5       String?
  lang      String?
  desc      String?
  groupId   String?
  click_flag Int?
  download_flag Int?
  @@map("EmojiInfoDesc")
}

model EmojiInfo {
  md5           String @id
  svrid         String?
  catalog       Int?
  type          Int?
  size          Int?
  start         Int?
  state         Int?
  name          String?
  content       String?
  reserved1     String?
  reserved2     String?
  reserved3     Int?
  reserved4     Int?
  app_id        String?
  groupId       String?
  lastUseTime   BigInt?
  framesInfo    String?
  idx           Int?
  temp          Int?
  source        Int?
  needupload    Int?
  designerID    String?
  thumbUrl      String?
  cdnUrl        String?
  encrypturl    String?
  aeskey        String?
  width         Int?
  height        Int?
  externUrl     String?
  externMd5     String?
  activityid    String?
  tpurl         String?
  tpauthkey     String?
  wxamMd5       String?
  attachedText  String?
  captureStatus Int?
  attachedEmojiMD5 Bytes?
  imitateMd5    String?
  captureUploadErrCode Int?
  captureUploadCounter Int?
  captureEnterTime BigInt?
  lensId        String?
  attachTextColor String?
  captureScene  Int?
  attr          String?
  linkId        String?
  meanings      String?
  custom_meaning String?
  emojiFlag     BigInt?
  @@map("EmojiInfo")
}

model ImgInfo2 {
  id           Int    @id
  msgSvrId     BigInt?
  offset       Int?
  totalLen     Int?
  bigImgPath   String?
  thumbImgPath String?
  createtime   Int?
  msglocalid   Int?
  status       Int?
  nettimes     Int?
  reserved1    Int?
  reserved2    Int?
  reserved3    String?
  reserved4    String?
  hashdthumb   Int?
  iscomplete   Int?
  origImgMD5   String?
  compressType Int?
  midImgPath   String?
  forwardType  Int?
  hevcPath     String?
  sendImgType  Int?
  originSourceMd5 String?
  msgTalker    String?
  @@map("ImgInfo2")
}